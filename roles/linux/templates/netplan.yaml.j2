network:
  version: 2
  renderer: networkd
  ethernets:
    {{ linux_ifname }}:
      dhcp4: false
      addresses:
        - {{ target_ip }}/24
      gateway4: {{ default_gateway | default('192.168.33.1') }}
      nameservers:
        addresses:
          - {{ dns_servers[0] | default('1.1.1.1') }}
          - {{ dns_servers[1] | default('1.0.0.1') }}
